<div class="container center">
 <div class="card-panel">
<div id="app">
  <h1>Flight Reservation</h1>
</div>

<!-- Display all the available flights -->
<script type="html/template" id="SearchViewTemplate">
<div id="searchForm">
  An instance of searchInputView should be loaded here
</div>

<div id="flights">
  <table>
    <thead>
      <tr>
        <th>Origin</th>
        <th>Destination</th>
        <th>Flight No</th>
        <th>Date</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody id="showFlight">
    </tbody>
  </table>

</div>
</script>

<!-- Search Flight -->
<script type="html/template" id="SearchInputTemplate">
  <input type="text" placeholder="origin" id="origin"/>
  <input type="text" placeholder="destination" id="destination"/>

  <button class="btn blue darken-4">Submit</button>
</script>

<script type="html/template" id="AppViewTemplate">
  <h1> Emus Backbone </h1>
</script>


<script type="html/template" id="FlightInfoTemplate">

   <p>Flight</p>
   
  <div id="info">
  <p id="origin"></p>
  <p id="destination"></p>
  <p id="fn"></p>
  <p id="dd"></p>

</script>


<!-- Includes above script in this SeatInputViewTemplate -->
<script type="html/template" id="SeatInputViewTemplate">

  <div id="info">

  </div>

  <div id="seats">

  <table name="mytab" id="mytab1">
  <thead>
  <tr>
  <th>A</th><th>B</th><th>Aisle</th><th>C</th><th>D</th>
  </tr>
  </thead>
  </table>

</div>

</script>
 </div>
</div>
